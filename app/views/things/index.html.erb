<h1 class="title" style="text-align: center; padding-bottom:20px">Search for 3D Models</h1>
<div class="ui fluid container" style="padding-top:20px; overflow: auto; height: 100%; min-height: 100%;">
  <div style="padding: 40px 0px;">
    <div id="results" class="ui fluid container">
      <div class="ui container" style="padding-bottom: 100px;"><br><br>
        <div class="ui centered cards">
          <% @things.each do |thing| %>
            <%= render "things/index_item", thing: thing %>
          <% end %>
        </div>
        <div class="ui centered cards">
          <%= paginate @paginatedthings %>
          
        </div>
        <br><br>
      </div>
    </div>
  </div>
</div>
